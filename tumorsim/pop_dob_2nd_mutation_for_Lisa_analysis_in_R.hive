-- **************************************************************************************************************;
-- this finds population and dob of second mutation (first after germ cell), for all detecable tumors for further Analysis by Lisa in R;
-- **************************************************************************************************************;

set hive.cli.print.header=true;
set hive.execution.engine=tez;

select substr(t.tumorid,1,3) as tumorclass 
,'  ', t.tumorid as tumorid
,'  ', t.dob as dob
,'  ', p.population as population
from
(
  select tumorid , dob 
  from tmp.tumors_detectable
  where subclone = 1
  and finalsnapshot = 'Y'
) t
inner join tmp.populations_detectable p
  on t.tumorid = p.tumorid
    and t.dob = p.generation
order by tumorclass , tumorid
;


